---
import ContentList, { ContentLink } from "../../components/ContentList.astro";
import BaseLayout from "../../layouts/BaseLayout.astro";

const title = "Stephen Cooper";
const description = "Latest content from Stephen Cooper, Senior Software Engineer at AG Grid in London.";
const permalink = `${Astro.site.href}content`;


import jsNation2023 from './jsnation-2023.png';
import reactUS from './ReactUS.png';


const contentItems: ContentLink[] = [
{
    name: "React Advanced 2023: Patterns for Performance",
    youTubeLink: "https://www.youtube.com/embed/qR608qHWTCA"
  },
  {
    name: "React Summit US 2023: Rendering: To Sync or Not to Sync",
    url: "https://portal.gitnation.org/contents/rendering-to-sync-or-not-to-sync",
    img: reactUS,
  },
  {
    name: "NG Tiny Conf 2023: Is your Component Ready for Strict Mode?",
    youTubeLink: "https://www.youtube.com/embed/hIMgpj3qR1E?si=Od6o2lhqgvIY8Yni",
  },
  {
    name: "Angular Community Meetup May 2023",
    youTubeLink: "https://www.youtube.com/embed/gA6iJbA9oBg?si=3UAvkq-awrV8wREl",
  },
  {
    name: "JS Nation 2023: Supercharged Code Refactoring via Abstract Syntax Trees",
    url: "https://portal.gitnation.org/contents/visualising-data-with-react-ag-grid-and-ag-charts",
    img: jsNation2023,
  },
  {
    name: "Angular Connect: ngTemplateOutlet: The Secret to Customisation",
    youTubeLink: "https://www.youtube.com/embed/2SnVxPeJdwE?start=43",
  },
  {
    name: "Dev Chat",
    youTubeLink: "https://www.youtube.com/embed/6UBj-L9Phqg?si=kSLPlmfOBHq5Rw3t",
  },
  {
    name: "AG Grid at ng-Conf 2022",
    youTubeLink: "https://www.youtube.com/embed/4dhBnvgb5H0?start=83",
  },
  {
    name: "JS Nation 2022: Auto maintaining thousands of code demos across multiple framework variations",
    youTubeLink: "https://www.youtube.com/embed/kOwAjLgSY14?start=41",
  },
  {
    name: "AG Grid Angular Tutorial Playlist",
    youTubeLink:
      "https://www.youtube.com/embed/videoseries?start=83&list=PLsZlhayVgqNw6VHFn4j6FcJM5vLACsf0x",
  },
  {
    name: "Angular Nation: Meet the Team",
    youTubeLink: "https://www.youtube.com/embed/MRy--gvA8kI?start=189",
